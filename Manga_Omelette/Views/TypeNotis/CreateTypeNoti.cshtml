@model Manga_Omelette.Models_Secondary.CreateTypeNotiViewModel

@{
    ViewBag.Title = "Create New Type Noti";
    Layout = "_SuperADMINLayout";
}

<form asp-controller="TypeNotis" asp-action="CreateTypeNoti" method="post" class="mt-3">
    <div asp-validation-summary="All" class="text-danger">
    </div>
    <div class="form-group row">
        <label asp-for="TypeName" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="TypeName" class="form-control" placeholder="Name">
            <span asp-validation-for="TypeName" class="text-danger"></span>
        </div>
    </div>

    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary" style="width:auto">
                Create Type
            </button>
        </div>
    </div>
</form>